<%= live_component @socket,
    PentoWeb.Live.Components.Title,
    title: @title %>

<ul>
<%= for list_item <- @list_items do %>
  <%= live_component @socket,
      PentoWeb.Live.Components.ListItem,
      content: list_item %>
<%= end %>
</ul>

<section class="row">

  <%= live_component @socket,
    PentoWeb.DemographicLive.FormComponent,
    user: @current_user %>
  
</section>